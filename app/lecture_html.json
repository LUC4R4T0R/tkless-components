{
  "main":[
    {
      "tag": "div",
      "class": "header"
    },
    {
      "tag": "div",
      "class": "content"
    },
    {
      "tag": "div",
      "class": "feedback",
      "onclick": "%fclick%",
      "inner": {
        "tag": "p",
        "inner": "feedback"
      }
    },
    {
      "tag": "div",
      "class": "footer"
    }
  ],

  "header": [
    {
      "tag": "div",
      "class": "home fa fa-home fa-2x",
      "onclick": "%hclick%"
    },
    {
      "tag": "div",
      "class": "position",
      "inner": "%pos%",
      "onclick": "%pclick%"
    },
    {
      "tag": "div",
      "class": "user"
    }
  ],

  "unit": [
    {
      "tag": "div",
      "class": "rubric",
      "inner":
      {
        "tag": "div",
        "class": "label",
        "inner": "%label%"
      }
    },
    {
      "tag": "div",
      "class": "parts"
    }

  ],

  "entry": {
    "tag": "div",
    "inner":
    [
      {
        "tag": "div",
        "class": "fa fa-%icon% fa-fw fa-%size%"
      },
      {
        "tag": "div",
        "inner": "%label%%lock%"
      },
      "%rating%"
    ]
  },

  "quizz_result": {
    "tag": "div",
    "class": "quizz_result",
    "inner":
    [
      {
        "tag": "div",
        "class": "title",
        "inner": "%title%"
      },
      {
        "tag": "div",
        "class": "text",
        "inner": "%text%"
      },
      {
        "tag": "div",
        "class": "button",
        "inner":
        {
          "tag": "button",
          "inner": "%button%",
          "onclick": "%click%"
        }
      }
    ]
  },

  "feedback": {
    "tag": "div",
    "class": "feedback",
    "inner": [
      {
        "tag": "div",
        "class": "head",
        "inner": [
          {
            "tag": "div",
            "class": "kind",
            "inner": "%kind%"
          },
          {
            "tag": "div",
            "class": "title",
            "inner": "%title%"
          }
        ]
      },
      {
        "tag": "div",
        "class": "text",
        "inner": "%text%"
      }
    ]
  }
}